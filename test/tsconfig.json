{
  "compilerOptions": {
    "types": ["node", "@wdio/globals/types", "@wdio/mocha-framework"],
    "outDir": "./built",
    "allowJs": true,
    "target": "ES6",
    "module": "Node16",
    "moduleResolution": "Node16",
    "allowImportingTsExtensions": true,
    "noEmit": true,
    "typeRoots": ["./helpers/types", "./node_modules"]
  },
  "include": ["./**/*"]
}
