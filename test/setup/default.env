# == TEST RUNNER CONFIG
TEST_LOG_LEVEL=debug
HEADED_TESTS=false
MOCHA_OPTS_TIMEOUT=90000
WAIT_FOR_TIMEOUT=30000

# == TEST SERVICES
# wikibase / wikibase-client / wikibase-jobrunner
WIKIBASE_IMAGE_NAME=wikibase
WIKIBASE_BUNDLE_IMAGE_NAME=wikibase-bundle
MW_ADMIN_NAME=WikibaseAdmin
MW_ADMIN_PASS=WikibaseDockerAdminPass
MW_ADMIN_EMAIL=admin@example.com
MW_WG_SECRET_KEY=secretkey
MW_SERVER=http://wikibase.svc
MW_CLIENT_SERVER=http://wikibase-client.svc
MW_SCRIPT_PATH=/w
MW_SITE_NAME=wikibase-docker
MW_SITE_LANG=en
MW_WG_JOB_RUN_RATE=2
WIKIBASE_PINGBACK=false
MAX_JOBS=4
MW_WG_ENABLE_UPLOADS=true
# mysql
DB_SERVER=mysql.svc:3306
DB_USER=wikiuser
DB_PASS=sqlpass
DB_NAME=my_wiki
# can be deprecated by moving defaultFunctions#dockerExec into TestEnvironment
# and using docker compose
DOCKER_MYSQL_NAME=wikibase-suite-test-services-mysql-1
# wdqs
WDQS_IMAGE_NAME=wdqs
WDQS_SERVER=wdqs.svc:9999
# wdqs-frontend
WDQS_FRONTEND_IMAGE_NAME=wdqs-frontend
WDQS_FRONTEND_SERVER=wdqs-frontend.svc
# wdqs-proxy
WDQS_PROXY_IMAGE_NAME=wdqs-proxy
WDQS_PROXY_SERVER=http://wdqs-proxy.svc
# quickstatements
QUICKSTATEMENTS_IMAGE_NAME=quickstatements
QS_SERVER=http://quickstatements.svc
# elasticsearch
ELASTICSEARCH_IMAGE_NAME=elasticsearch
MW_ELASTIC_HOST=elasticsearch.svc
MW_ELASTIC_PORT=9200
