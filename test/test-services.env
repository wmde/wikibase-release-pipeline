MEDIAWIKI_VERSION=1.40.1

# == TEST SERVICES
# wikibase / wikibase-client / wikibase-jobrunner
WIKIBASE_SUITE_WIKIBASE_IMAGE_URL=wikibase/wikibase
WIKIBASE_SUITE_WIKIBASE_BUNDLE_IMAGE_URL=wikibase/wikibase-bundle
MW_ADMIN_NAME=WikibaseAdmin
MW_ADMIN_PASS=WikibaseDockerAdminPass
MW_ADMIN_EMAIL=admin@example.com
MW_WG_SECRET_KEY=secretkey
WIKIBASE_HOST=wikibase.svc
MW_SERVER=http://wikibase.svc
MW_CLIENT_SERVER=http://wikibase-client.svc
MW_SCRIPT_PATH=/w
MW_SITE_NAME=wikibase-docker
MW_SITE_LANG=en
MAX_JOBS=0
MW_WG_JOB_RUN_RATE=2
WIKIBASE_PINGBACK=false
MW_WG_ENABLE_UPLOADS=true
# mysql
DB_SERVER=mysql.svc:3306
DB_USER=wikiuser
DB_PASS=sqlpass
DB_NAME=my_wiki
# wdqs
WIKIBASE_SUITE_WDQS_IMAGE_URL=wikibase/wdqs
WDQS_SERVER=http://wdqs.svc:9999
# wdqs-frontend
WIKIBASE_SUITE_WDQS_FRONTEND_IMAGE_URL=wikibase/wdqs-frontend
WDQS_FRONTEND_SERVER=http://wdqs-frontend.svc
# wdqs-proxy
WIKIBASE_SUITE_WDQS_PROXY_IMAGE_URL=wikibase/wdqs-proxy
WDQS_PROXY_SERVER=http://wdqs-proxy.svc
# quickstatements
WIKIBASE_SUITE_QUICKSTATEMENTS_IMAGE_URL=wikibase/quickstatements
QS_SERVER=http://quickstatements.svc
# elasticsearch
WIKIBASE_SUITE_ELASTICSEARCH_IMAGE_URL=wikibase/elasticsearch
MW_ELASTIC_HOST=elasticsearch.svc
MW_ELASTIC_PORT=9200
